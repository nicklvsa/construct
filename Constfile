var name = @COMPUTERNAME
var example = 25

run (arg0, opt arg1) {
    var cool = &example
    var test = &cool
    $ python run.py arg0 arg1 &name
    $ python run.py &name &test
}

_ {
    $ echo import sys > run.py
    $ echo print('&name') >> run.py
    $ echo print(sys.argv[1:]) >> run.py
}