var name = @COMPUTERNAME
var example = 25

another {
    echo test
    echo &name
}

prerun (arg0) {
    $ python run.py arg0 abc 123
}

run (arg0, opt arg1) < prerun, another {
    var cool = &example
    var test = &cool
    $ echo &prerun.0 &another.0 &another.1
    $ python run.py arg0 arg1 &name &test
}

_ {
    $ echo import sys > run.py
    $ echo print(sys.argv[1:]) >> run.py
}